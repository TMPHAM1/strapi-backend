{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "seats_available": {
      "type": "integer"
    },
    "location": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date"
    },
    "teacher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::teacher.teacher",
      "inversedBy": "courses"
    },
    "user": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "courses"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance-record.attendance-record",
      "mappedBy": "course"
    }
  }
}
